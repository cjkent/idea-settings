<templateSet group="Rust">
  <template name="str" value="$TYPE$ { $END$ }" description="struct body" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="fn" value="fn $NAME$($ARGS$) -&gt; $RETURN_TYPE$ {&#10;    $END$&#10;}" description="function definition" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="db" value="{:?}" description="debug anchor" toReformat="false" toShortenFQNames="true" />
  <template name="tst" value="#[test]&#10;fn $NAME$() {&#10;    $END$&#10;}" description="test fn" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="l1" value="|$ARG$| $END$" description="single arg lambda" toReformat="false" toShortenFQNames="true">
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="l2" value="|$ARG1$, $ARG2$| $END$" description="two arg lambda" toReformat="false" toShortenFQNames="true">
    <variable name="ARG1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARG2" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
</templateSet>