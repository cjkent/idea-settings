<application>
  <component name="ActionMacroManager">
    <macro name="split string literal">
      <action id="EditorPasteSimple" />
      <action id="EditorLeft" />
      <action id="EditorLeft" />
      <action id="EditorLeft" />
      <action id="EditorLeft" />
    </macro>
    <macro name="append dot to line">
      <action id="EditorLineEnd" />
      <typing text-keycode="46:0">.</typing>
    </macro>
    <macro name="Reformat Line">
      <action id="EditorLineEnd" />
      <action id="EditorLineStartWithSelection" />
      <action id="ReformatCode" />
    </macro>
    <macro name="right comma space">
      <action id="EditorRight" />
      <typing text-keycode="44:0;32:0">,&amp;#x20;</typing>
    </macro>
    <macro name="right space plus space">
      <action id="EditorRight" />
      <typing text-keycode="32:0;61:1;32:0">&amp;#x20;+&amp;#x20;</typing>
    </macro>
    <macro name="right space">
      <action id="EditorRight" />
      <typing text-keycode="32:0">&amp;#x20;</typing>
    </macro>
    <macro name="join lines, delete">
      <action id="EditorJoinLines" />
      <action id="EditorDelete" />
    </macro>
    <macro name="Newline with line comment">
      <action id="EditorEnter" />
      <typing text-keycode="47:0;47:0;32:0">//&amp;#x20;</typing>
    </macro>
    <macro name="Type underscore">
      <typing text-keycode="0:1">_</typing>
    </macro>
    <macro name="Convert Javadoc single to multiple">
      <action id="EditorLineStart" />
      <action id="EditorLineStart" />
      <action id="EditorNextWord" />
      <action id="EditorNextWord" />
      <action id="EditorEnter" />
      <action id="EditorLineEnd" />
      <action id="EditorLeft" />
      <action id="EditorLeft" />
      <action id="EditorBackSpace" />
      <action id="EditorEnter" />
      <action id="EditorBackSpace" />
      <action id="EditorBackSpace" />
      <action id="EditorLineEnd" />
    </macro>
    <macro name="Type underscore">
      <typing text-keycode="0:1">_</typing>
    </macro>
  </component>
</application>